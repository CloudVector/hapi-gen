/*global window */
"use strict";

window.{namespace} = window.{namespace} || {~lb}{~rb};

// Create global log object
window.{namespace}.log = {
    error: function (err) {
        if (console) {
            console.error(err);
        }
    },
    info: function (obj) {
        if (console) {
            console.log(obj);
        }
    }
};
